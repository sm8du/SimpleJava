class Person {
	String name; 
	int age;
	
	
	int calculateYear() {
		int yearsLeft = (65-age);
		System.out.println(yearsLeft);
		
		return yearsLeft;
	}
	
}




public class Getters {
	
	public static void main(String[] args) {
		
		Person person1 = new Person();
		
		person1.name = "Stefan";
		person1.age = 23;
		
		
		person1.calculateYear();
	}
}
